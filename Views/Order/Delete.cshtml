@model Order
<h1>Delete</h1>
<h5>Are you sure you want to delete this?</h5>
<hr>
<dl class="row">
    <dt class = "col-sm-2">
        Room Number
    </dt>
    <dd class = "col-sm-10">
        @Model.RoomNum
    </dd>
    <dt class = "col-sm-2">
        Price
    </dt>
    <dd class = "col-sm-10">
        @Model.Price
    </dd>
    <dt class = "col-sm-2">
        Quantity
    </dt>
    <dd class = "col-sm-10">
        @Model.Quantity
    </dd>
    
</dl>
<form asp-action="DeleteConfirmed">
    <div class="form-group">
        <input asp-for="Id" type="hidden" />
        <button type="submit" class="btn btn-danger">Delete</button>
    </div>
</form>